<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.relearn.mobileapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileApp">
        <activity
            android:name="com.relearn.mobileapp.AboutUs"
            android:exported="false" />
        <activity
            android:name="com.relearn.mobileapp.WelcomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.relearn.mobileapp.NotesDashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.relearn.mobileapp.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.relearn.mobileapp.SubjectDashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.relearn.mobileapp.Details"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.relearn.mobileapp.Login"
            android:screenOrientation="portrait" />
        <activity android:name="com.relearn.mobileapp.PdfActivity" />
    </application>

</manifest>